{
  "name": "@coredomains/cns-namehash",
  "version": "0.1.0",
  "description": "CoreDAO Domains namehash utility",
  "homepage": "https://github.com/coredaodomains/core-monorepo/blob/main/packages/cns-namehash/README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/coredaodomains/core-monorepo.git"
  },
  "main": "dist/cjs/index.js",
  "types": "dist/esm/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "files": [
    "lib/**/*",
    "!src/**/*.test.js",
    "!src/**/__tests__"
  ],
  "scripts": {
    "clean": "rm -rf dist/*",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build": "pnpm run build:esm && pnpm run build:cjs",
    "test": "jest --config jest.config.js --coverage",
    "pretty": "prettier --write \"src/**/*.(ts|js)\"",
    "lint:fix": "eslint src --ext .js,ts"
  },
  "keywords": [
    "CoreDAO",
    "CNS",
    "typescript"
  ],
  "author": "CoreDAO Team",
  "contributors": [
    {
      "name": "lanbery",
      "email": "lanbery@gmail.com",
      "url": "https://github.com/lanbery"
    }
  ],
  "bugs": {
    "url": "https://github.com/coredaodomains/core-monorepo/issues"
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "@types/node": "^18.13.0",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "eslint": "^8.33.0",
    "eslint-plugin-jest": "^27.2.1",
    "idna-uts46-hx": "^4.1.2",
    "jest": "^29.4.2",
    "js-sha3": "^0.8.0",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.5"
  },
  "peerDependencies": {
    "eslint": "^8.33.0",
    "jest": "^29.4.2",
    "typescript": "^4.9.5"
  }
}
